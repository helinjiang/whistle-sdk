{
  "name": "whistle-sdk",
  "version": "0.1.0",
  "description": "SDK for whistle",
  "main": "lib/index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc -d",
    "test": "npm run test:unit",
    "test:unit": "mocha 'test/**/*.test.{ts,tsx}'",
    "test:cover": "nyc npm run test:unit",
    "lint": "eslint src test --ext .ts"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helinjiang/whistle-sdk.git"
  },
  "keywords": [
    "whistle",
    "node",
    "SDK"
  ],
  "author": "helinjiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helinjiang/whistle-sdk/issues"
  },
  "homepage": "https://github.com/helinjiang/whistle-sdk#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/expect": "^24.3.0",
    "@types/fs-extra": "^9.0.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.5",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.1.0",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "cmd-hub": "^0.1.0",
    "fs-extra": "^10.0.0"
  }
}
